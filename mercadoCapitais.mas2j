MAS mercadoCapitais {
	
	//define infraestrutura
    infrastructure: Centralised
	
	//define ambiente
	environment: src.mas.MercadoEnvironment
	
	//define controle de execução
	//executionControl: src.mas.MercadoController
	
	//define agentes
    agents:
        bovespa agentClass src.mas.BovespaAgent;
        investidor#1 agentClass src.mas.InvestidorAgent;
}
